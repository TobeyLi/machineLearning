   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "plt.style.use('seaborn-white')\n",
    "%matplotlib inline"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 78,
   "metadata": {},
   "outputs": [],
   "source": [
    "weights 是一个列表，里面存储的是我们用来产生随机数据的那条直线的法向量。\n"
    "weights=[4,3]\n",
    "numLines=100"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 79,
   "metadata": {},
   "outputs": [],
   "source": [
    "w=np.array(weights)\n",
    "num_features=len(weights)\n",
    "dataset=np.zeros((numLines,num_features+1))\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 80,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(numLines):\n",
    "    x=np.random.rand(1,num_features)*20-10\n",
    "    # 计算內积\n",
    "    inner_product=np.sum(w*x)\n",
    "    # 小于0则是代表负实例，大于0则是代表正实例\n",
    "    if inner_product<=0:\n",
    "        dataset[i]=np.append(x,-1)\n",
    "    else:\n",
    "        dataset[i]=np.append(x,1)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 81,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "<matplotlib.legend.Legend at 0x393f690>"
      ]
     },
     "execution_count": 81,
